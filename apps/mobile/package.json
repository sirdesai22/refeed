{
  "name": "@refeed/mobile",
  "version": "0.1.0",
  "main": "index.ts",
  "types": "index.ts",
  "packageManager": "pnpm@8.9.2",
  "engines": {
    "node": ">=v18.17.1"
  },
  "private": true,
  "scripts": {
    "dev": "pnpm with-env expo start --dev-client",
    "clean": "rm -rf .expo .turbo node_modules",
    "dev:ios": "pnpm with-env expo start --ios --lan --dev-client",
    "dev:android": "pnpm with-env expo start --android --lan --dev-client",
    "dev:ios:build": "pnpm with-env eas build --platform ios --profile development",
    "dev:android:build": "pnpm with-env eas build --platform android --profile development",
    "dev:android:build:local": "pnpm with-env eas build --platform android --profile development --local",
    "prod:android:build": "pnpm with-env eas build --platform ios --profile production",
    "prod:ios:build": "pnpm with-env eas build --platform ios --profile production",
    "devclear": "pnpm with-env expo start --android --lan --clear",
    "submit:ios": "pnpm with-env eas submit --platform ios --profile production",
    "preview": "PRODUCTION=1 pnpm with-env expo start --no-dev --minify",
    "format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
    "lint": "pnpm eslint .",
    "typecheck": "tsc --noEmit",
    "build:android": "pnpm with-env eas build --platform android --profile production",
    "android": "pnpm with-env expo run:android",
    "with-env": "dotenv -e ../../.env --",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@animatereactnative/marquee": "^0.2.0",
